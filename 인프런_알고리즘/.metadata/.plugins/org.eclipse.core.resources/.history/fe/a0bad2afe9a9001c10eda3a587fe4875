import java.util.Scanner;

public class 문장속단어 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		String str = sc.nextLine();

		String ans = "";
		String prev = "";
		for(int i=0; i<str.length(); i++) {
			if(str.charAt(i) == 32) {
				ans = ans.length() > prev.length() ? ans : prev;
				prev = "";
			}
			prev += str.charAt(i);
		}
	}

}

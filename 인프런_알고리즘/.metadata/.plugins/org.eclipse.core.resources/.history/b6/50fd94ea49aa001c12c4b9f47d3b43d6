import java.util.Scanner;

public class 회문문자열 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		String str = sc.next();
		int front = 0;
		int back = str.length()-1;
		
		while(back > front) {
			if(str.charAt(front) == str.charAt(back)) {
				front += 1;
				back -= 1;
			}
		}
	}

}
